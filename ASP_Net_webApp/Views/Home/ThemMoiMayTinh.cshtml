
@{
    ViewBag.Title = "ThemMoiMayTinh";
    Layout = "~/Views/Shared/LayoutUser.cshtml";
}

<main>
    <div class="container">
        <div class="page-select feature" style="margin: 15px 0px">
            <h4>Thêm Mới Danh Sách</h4>
            <p>
                <a href="~/">
                    Quay Lại Trang Home
                </a>
            </p>
            <form action="~/Home/LuuThemMoiMayTinh" method="post">
                <div class="form-group">
                    <label for="email">Mã Máy</label>
                    <input type="text" class="form-control" name="MaMay">
                </div>
                <div class="form-group">
                    <label for="pwd">Dòng Máy</label>
                    <input type="text" class="form-control" name="dongMay">
                </div>
                <div class="form-group">
                    <label for="pwd">Giá Bán</label>
                    <input type="number" class="form-control" name="GiaBan">
                </div>
                <div class="form-group">
                    <label for="pwd">Ngày Sản xuất</label>
                    <input type="date" class="form-control" name="NgaySanXuat">
                </div>
                <div class="form-group">
                    <label for="pwd">Hãng Sản xuất</label>
                    <input type="text" class="form-control" name="HangSanXuat">
                </div>

                <button type="submit" class="btn btn-default">Submit</button>
            </form>
        </div>
    </div>
</main>

